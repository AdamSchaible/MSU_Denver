/parallelogramflower
{
gsave
2 inch 2 inch translate parallelogramblue
30 rotate
parallelogramred
30 rotate
parallelogramgreen
30 rotate
parallelogramdarkblue
30 rotate
parallelogrammagenta
30 rotate
parallelogramyellow
30 rotate
parallelogramorange
30 rotate
parallelogrambrown
30 rotate
parallelogramwhite
30 rotate
parallelogramdarkgreen
30 rotate
parallelogrampurple
30 rotate
parallelogramblack
grestore
} def
parallelogramflower
gsave
1.5 inch 1.5 inch translate
1.5 1.5 scale
parallelogramflower
grestore

gsave
0 inch 5 inch translate
2 2 scale
parallelogramflower
grestore

